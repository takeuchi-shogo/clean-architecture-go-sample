# 自分用に
alias dceb = 'docker-compose --env-file ./.env -f docker/docker-compose.yml build'
alias dcec = 'docker-compose --env-file ./.env -f docker/docker-compose.yml config'
alias dceup = 'docker-compose --env-file ./.env -f docker/docker-compose.yml up'
alias dced = 'docker-compose --env-file ./.env -f docker/docker-compose.yml down'
